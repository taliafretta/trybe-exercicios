def bigger(number, other):
  if other > number:
    return other
  return number

def mean(numbers):
  sum = 0
  for number in numbers:
    sum += number
  return sum / len(numbers)

def draw_square(size):
  for i in range(size):
    print(size * "*")

def find_biggest_name(names):
  biggest_name = names[0]
  for name in names:
    if len(name) > len(biggest_name):
      biggest_name = name
  return biggest_name